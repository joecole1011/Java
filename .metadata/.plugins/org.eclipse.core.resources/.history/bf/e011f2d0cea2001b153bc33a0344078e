package com.kimzun.helloWord;

public class HelloWorld {

	public static void main(String[] args) {
		int a = 34;
		boolean b = true;
		String c = "Akawit" ;
		double d = 15.55;
		byte e = 2;
		System.out.println("This is a Integers" + a );
		System.out.println("This is a Boolean" + b );
		System.out.println("This is a String" + c );
		System.out.println("This is a Floating-Point" + d );
		System.out.println("This is a Integers" + e );		
	}

}
