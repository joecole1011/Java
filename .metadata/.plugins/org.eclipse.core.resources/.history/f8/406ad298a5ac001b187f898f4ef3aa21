package com.kimzun.AbstractEnumFinal;

public class Enum {
	enum Status {
		GOOD, BAD, STILL
	}

	public static void main(String[] args) {
		Status myStatus = Status.STILL;
		switch (myStatus) {
		case GOOD:
			System.out.println("พร้อมจะลุยกันมานานแล้วววว!!");
			break;
		case BAD:
			System.out.println("ง่วงจังขอกาแฟหน่อย");
			break;
		case STILL:
			System.out.println("ไม่ไหวแล้วไปนอนละบายยย");
			break;
		}
	}
}
