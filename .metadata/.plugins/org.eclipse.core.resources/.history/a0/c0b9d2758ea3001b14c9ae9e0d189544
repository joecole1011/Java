package com.kimzun.helloWord;

public class Lab3 {

	public static void main(String[] args) {
		int unaryInt  = 20;
		System.out.println("++");
		
		unaryInt++;
		System.out.println("Result " + unaryInt);
		
		unaryInt++;
		System.out.println("Result " + unaryInt);
		
		unaryInt++;
		System.out.println("Result " + unaryInt);
		
		unaryInt++;
		System.out.println("Result " + unaryInt);
		
		unaryInt++;
		System.out.println("Result " + unaryInt);
		
		System.out.println();
		System.out.println("--");
		
		unaryInt--;
		System.out.println("Result " + unaryInt);

		unaryInt--;
		System.out.println("Result " + unaryInt);
		
		unaryInt--;
		System.out.println("Result " + unaryInt);
		
		unaryInt--;
		System.out.println("Result " + unaryInt);
		
		unaryInt--;
		System.out.println("Result " + unaryInt);
		
		System.out.println();
		System.out.println("&&");
		float value_1 = 1;
		float value_2 = 2;
		if ((value_1 == 1) && (value_2 == 2))
			System.out.println("value1 is 1 AND value2 is 2");
		if ((value_1 == 1) || (value_2 == 1))
			System.out.println("value1 is 1 OR value2 is 1");
		System.out.println();
		System.out.println("|| ");
		char value_1_1 = 1;
		char value_2_1 = 2;
		if ((value_1_1 == 1) && (value_2_1 == 2))
			System.out.println("value1 is 1 AND value2 is 2");
		if ((value_1 == 1) || (value_2_1 == 1))
			System.out.println("value1 is 1 OR value2 is 1");
		
		}
}
