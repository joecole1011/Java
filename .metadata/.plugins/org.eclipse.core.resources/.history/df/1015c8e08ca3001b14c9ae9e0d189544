package com.kimzun.helloWord;

public class Lab3 {

	public static void main(String[] args) {
		int unaryInt  = 5;
		System.out.println("Result " + unaryInt);
		unaryInt++;
		System.out.println("Result " + unaryInt);
		unaryInt++;
		System.out.println("Result " + unaryInt);
		unaryInt = -unaryInt;
		System.out.println("Result " + unaryInt);

	}

}
