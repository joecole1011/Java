package com.kimzun.helloWord;

public class Lab3 {

	public static void main(String[] args) {
		int  = 5;
		System.out.println("Result " + unaryVar);
		unaryVar--;
		System.out.println("Result " + unaryVar);
		unaryVar++;
		System.out.println("Result " + unaryVar);
		unaryVar = -unaryVar;
		System.out.println("Result " + unaryVar);

	}

}
