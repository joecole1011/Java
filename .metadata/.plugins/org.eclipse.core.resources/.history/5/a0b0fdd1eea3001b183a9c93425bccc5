package com.kimzun.helloWord;

public class Lab5 {

	public static void main(String[] args) {
		int number = 20;
		switch (number) {
		case 80:
		System.out.println("Gread A"); break;
		case 70:
		System.out.println("Gread B"); break;
		case 60:
		System.out.println("30"); break;
		default:
		System.out.println("Not in 10, 20 or 30");
		}

	}

}
